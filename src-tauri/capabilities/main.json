{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main",
  "description": "Capability for the main window",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "window:default",
    "window:allow-center",
    "window:allow-maximize",
    "window:allow-minimize",
    "window:allow-close",
    "window:allow-start-dragging",
    "window:allow-toggle-maximize"
  ]
}